    "core-js": "^2.4.1",
    "cors": "^2.8.1",
    "express": "^4.13.4",
    "express-jwt": "^3.3.0",
    "fancybox": "^3.0.0",
    "font-awesome": "^4.7.0",
    "google-spreadsheet": "^2.0.3",
    "jquery": "^3.1.1",
    "jquery-ui": "^1.12.1",
    "jsonwebtoken": "^5.7.0",
    "modernizr": "^3.3.1",
    "nivo-slider": "^3.2.0",
    "node-uuid": "^1.4.7",
    "normalize.css": "^5.0.0",
    "owl.carousel": "^2.2.0",
    "path": "^0.12.7",
